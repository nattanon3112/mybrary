<h2>All Book</h2>
<form action="/books" method="GET">
    <div>
        <label for="">Title</label>
        <input type="text" name="title" 
        value="<%= searchOptions.title %>">
        <button type="submit">Search</button>
    </div>
    <div>
        <label for="">Publish After</label>
        <input type="text" name="publishAfter" 
        value="<%= searchOptions.publishAfter %>">
        <button type="submit">Search</button>
    </div>
    <div>
        <label for="">Publish Before</label>
        <input type="text" name="publishBefore" 
        value="<%= searchOptions.publishBefore %>">
        <button type="submit">Search</button>
    </div>
</form>
<br>
<div>
    <% books.forEach(book => { %>
        <img src="<%= book.coverImagePath %>" 
        alt="" height="150" width="100">
    <% }) %>
</div>
